{
  "name": "react-redux-popup",
  "version": "0.7.0",
  "description": "React redux popup",
  "main": "lib/react-redux-popup.js",
  "scripts": {
    "clean": "rimraf coverage lib test-reports",
    "build": "NODE_ENV=production webpack --config ./configs/webpack.config.js --progress --profile --colors",
    "coverage": "./node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint 'src/**' --cache",
    "prepublish": "npm run clean && npm run build",
    "start": "node ./example-app/server.js",
    "test": "./node_modules/jest/bin/jest.js",
    "test-dev": "./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^rrp": "<rootDir>/src"
    },
    "testRegex": "__tests__/.*.js$|__tests__/.*.jsx$"
  },
  "keywords": [],
  "author": "Ryan Kim",
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-require": "^2.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "eslint": "^3.6.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jest": "^15.1.1",
    "lodash": "^4.13.1",
    "node-sass": "^3.10.0",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.0"
  }
}
